<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%- include('setup') %>
  <link rel="stylesheet" href="styles/shop.css">
  <title>Shop - Barbie Card Shop</title>
</head>
<body>
  <%- include('navbar', { isLogin: true }) %>
  <section>
    <h3>Shop</h3>
    <div class="card-available">
      <% cards.forEach(card => { %>
        <div class="card">
          <img src="<%= card.image %>" alt="">
        </div>
      <% }); %>
    </div>
  </section>
</body>
</html>